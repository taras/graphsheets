{
  "name": "graphsheets",
  "version": "0.1.0",
  "description": "GraphQL interface for Google Sheets API",
  "engines": {
    "node": ">=7.4"
  },
  "bin": {
    "graphsheets": "./bin/graphsheets"
  },
  "main": "lib/cli.js",
  "scripts": {
    "pretest": "npm run compile",
    "test": "npm run mocha",
    "mocha": "mocha test/ --trace-warnings --recursive -t 10000 -S -R spec --require source-map-support/register --require intelli-espower-loader",
    "compile": "tsc",
    "prepublish": "npm run compile",
    "precommit": "lint-staged",
    "clean": "find . -name '*.js.map' -type f -delete && find -E . -regex '\\./(test|lib)/.*.js$' -type f -delete"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/this-dot/graphsheets.git"
  },
  "keywords": [
    "google-sheets",
    "graphql",
    "spreadsheets"
  ],
  "author": "Taras Mankovski <tarasm@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/this-dot/graphsheets/issues"
  },
  "homepage": "https://github.com/this-dot/graphsheets#readme",
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/fs-extra": "^3.0.3",
    "@types/graphql": "^0.9.3",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.2",
    "@types/power-assert": "^1.4.29",
    "@types/yargs": "^6.6.0",
    "body-parser": "^1.17.2",
    "dataloader": "^1.3.0",
    "express": "^4.15.3",
    "find-up": "^2.1.0",
    "fs-extra": "^3.0.1",
    "google-auth-library": "^0.10.0",
    "googleapis": "^19.0.0",
    "graphql": "^0.10.3",
    "graphql-server-express": "^1.0.0",
    "graphql-tools": "^1.1.0",
    "heimdalljs-logger": "^0.1.9",
    "intelli-espower-loader": "^1.0.1",
    "jasmine": "^2.6.0",
    "jest-mock": "^20.0.3",
    "lodash.map": "^4.6.0",
    "lodash.zipobject": "^4.1.3",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.4",
    "prettier": "^1.5.0",
    "rsvp": "^3.6.1",
    "short-uuid": "^2.3.3",
    "typescript": "^2.3.4",
    "util.promisify": "^1.0.0",
    "yargs": "^8.0.2"
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "request-promise-native": "^1.0.4"
  }
}
